{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "active": {
            "type": "boolean"
        },
        "city": {
            "type": "string"
        },
        "contracts": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "currency": {
                            "type": "string"
                        },
                        "id": {
                            "type": "integer"
                        },
                        "price": {
                            "type": "number"
                        },
                        "type": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "currency",
                        "id",
                        "price",
                        "type"
                    ]
                }
            ]
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "id": {
            "type": "integer"
        },
        "name": {
            "type": "string"
        },
        "street": {
            "type": "string"
        },
        "surname": {
            "type": "string"
        },
        "zip": {
            "type": "string"
        }
    },
    "required": [
        "active",
        "city",
        "contracts",
        "email",
        "id",
        "name",
        "street",
        "surname",
        "zip"
    ]
}